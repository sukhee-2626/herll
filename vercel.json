{
    "redirects": [
        {
            "source": "/contact-us",
            "destination": "/contact.html",
            "permanent": false
        },
        {
            "source": "/about",
            "destination": "/about-us.html",
            "permanent": false
        },
        {
            "source": "/approach",
            "destination": "/our-approach.html",
            "permanent": false
        },
        {
            "source": "/where-we-fit",
            "destination": "/where-we-fit.html",
            "permanent": false
        }
    ],
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                }
            ]
        }
    ]
}